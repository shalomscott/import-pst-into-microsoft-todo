{
  "name": "import-pst-into-microsoft-todo",
  "version": "0.1.0",
  "description": "A NodeJS script that scans a .pst file containing tasks (legacy) and imports them into Microsoft Todo using the Microsoft Graph API.",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "devDependencies": {
    "@types/node": "~12.12",
    "@typescript-eslint/eslint-plugin": "~3.9.1",
    "@typescript-eslint/parser": "~3.9.1",
    "eslint": "~7.7.0",
    "eslint-config-prettier": "~6.11.0",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "tsutils": "~3.17.0",
    "typescript": "~4.0.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "dev": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "author": "Shalom Steinbach",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "~2.0.1"
  }
}
