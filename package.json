		"dotenv": "^8.2.0",
		"isomorphic-fetch": "^3.0.0",
		"pst-extractor": "^1.7.0",
		"ts-node": "^9.0.0",
		"tslib": "^2.0.1",
		"tsutils": "^3.17.0",
		"typescript": "^4.0.3",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@types/node": "^14.11.2",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
